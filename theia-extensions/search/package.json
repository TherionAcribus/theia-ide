{
  "private": true,
  "name": "theia-ide-search-ext",
  "version": "1.67.100",
  "description": "GeoApp Search - Recherche in-page r√©utilisable pour widgets Theia",
  "keywords": [
    "theia-extension"
  ],
  "license": "MIT",
  "files": [
    "lib",
    "src"
  ],
  "main": "lib/browser/search-frontend-module.js",
